{
	"collectCoverage": true,
	"testEnvironment": "jsdom",
	"verbose": false,
	"bail": 1,
	"moduleFileExtensions": ["js", "vue"],
	"setupFiles": [
		"<rootDir>/jest.init.js"
	],
	"moduleNameMapper": {
		"^~/(.*)$": "<rootDir>/src/$1"
	},
	"setupFilesAfterEnv": ["jest-extended/all"],
	"transform": {
		"^.+\\.js$": "babel-jest",
		"^.+\\.vue$": "vue3-jest"
	},
	"testRegex": "((\\.|/*.)(test))\\.js?$"
}